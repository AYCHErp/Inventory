@using MixERP.Inventory.Extensions
@model long

@{
    ViewBag.Title = "Checklist";
    Layout = ViewBag.InventoryLayoutPath;
}

@Html.FinancePartialView("Shared/Checklist.cshtml", TenantConvention.GetTenant())

<script>
    window.overridePath = "/dashboard/inventory/tasks/inventory-transfers";

    window.prepareChecklist({
        Title: "Inventory Transfer Checklist #@Model",
        ViewText: "View Transfers",
        ViewUrl: "/dashboard/inventory/tasks/inventory-transfers",
        AddNewText: "Add New Transfer",
        AddNewUrl: "/dashboard/inventory/tasks/inventory-transfers/new",
        ReportPath: "/dashboard/reports/source/Areas/MixERP.Inventory/Reports/Transfer.xml?transaction_master_id=@Model"
    });
</script>